{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
      "max-len": 0,
      "no-plusplus": 0,
      "class-methods-use-this": 0,
      "object-curly-newline": 0,
      "no-console": 0,
      "no-shadow": 0
    }
}
